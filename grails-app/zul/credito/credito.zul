                                            <listcell label="@load(each.taxas)"/>
                                            <listcell
                                                    label="@load(each.valorEmDivida)@converter('formatedNumber', format='###,##0.00')"/>


                                        </listitem>
                                    </template>

                                </listbox>
                            </tabpanel>
