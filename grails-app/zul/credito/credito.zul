                                                   colspan="3"/>
                                        <auxheader style="font-size:12pt;text-align:left"
                                                   label="@load(vm.credito.numeroDoCredito)"
                                                   colspan="1"/>

                                        <auxheader style="font-size:12pt;text-align:right" label="ID:" colspan="1"/>
                                        <auxheader style="font-size:12pt;text-align:left" label="@load(vm.credito.id)"
                                                   colspan="2"/>

                                    </auxhead>
                                    <listhead sizable="true">
                                        <listheader sort="auto(id)" width="80px" label="ID" align="left"/>
                                        <listheader sort="auto(descricao)" width="" label="Descrição" align="left"/>
                                        <listheader sort="auto(valorDaPrestacao)" width="100px" label="V. Prestação"
                                                    align="left"/>
                                        <listheader sort="auto(totalEmDivida)" width="80px" label="T. em Dívida"
                                                    align="left"/>
                                        <listheader sort="auto(valorDeJurosDeDemora)" width="100px"
                                                    label="V. de J. de mora"
                                                    align="left"/>
                                        <listheader sort="auto(valorPago)" width="100px" label="V. Pago" align="left"/>
                                        <listheader sort="auto(valorDeJuros)" width="100px" label="V. De Juros"
