                                            <listcell
                                                    label="@load(each.valorDeJurosDeDemora)@converter('formatedNumber', format='###,##0.00')"/>
                                            <listcell
                                                    label="@load(each.valorPago)@converter('formatedNumber', format='###,##0.00')"/>
                                            <listcell
